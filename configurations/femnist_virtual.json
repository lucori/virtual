{
    "session":{
        "num_gpus": 1
    },
    "data_set_conf":{
        "name": "femnist",
        "num_clients": 1
    },
    "training_conf": {"method": "virtual",
                     "tot_epochs_per_client": 200000,
                     "fed_avg_init": false,
                     "tensorboard_updates": 1,
                     "damping_factor": 1.0
    },
   "model_conf": {
              "layers": [{
                "input_shape": [784],
                "name": "DenseReparametrizationShared",
                "units": 100,
                "activation": "relu"
              },
                {
                "name": "DenseReparametrizationShared",
                "units": 100,
                "activation": "relu"
              },
                {
                "name": "DenseReparametrizationShared",
                "units": 10,
                "activation": "softmax"
              }],
              "prior_scale": 1.0
   },
   "hp": {"epochs_per_round": [1],
         "learning_rate": [0.001],
         "kl_weight": [1e-7],
         "batch_size": [20],
         "clients_per_round": [1],
         "hierarchical": [false],
         "optimizer": ["adam"],
          "scale_init": [[-6, 0.1]]
