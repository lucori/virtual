{
    "session":{
        "num_gpus": 1,
        "verbose": 0
    },
    "data_set_conf":{
        "name": "femnist",
        "num_clients": 100,
        "server_learning_rate": 0.1,
        "shape": [1, 28, 28]
    },
    "training_conf": {"method": "virtual",
                     "tot_epochs_per_client": 10000,
                      "clients_per_round": 10,
                      "epochs_per_round": 1,
                     "batch_size": 20,
                     "optimizer": "sgd",
                     "fed_avg_init": false,
                     "natural_lr_mult": 0.000000081,
                     "tensorboard_updates": 1
    },
   "model_conf": {"layers": [{"name": "Conv2DVirtualNatural",
                              "input_shape": [1, 28, 28],
                              "filters": 32,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu",
                              "bias_posterior_fn": null
                            },
                              { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                              {"name": "Conv2DVirtualNatural",
                              "input_shape": [1, 28, 28],
                              "filters": 64,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu",
                                "bias_posterior_fn": null
                            },
                            { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                            { "name": "Flatten"
                            },
                            { "name": "DenseLocalReparametrizationNaturalShared",
                              "units": 100,
                              "activation": "relu",
                              "bias_posterior_fn": null
                            },
                            { "name": "DenseLocalReparametrizationNaturalShared",
                              "units": 100,
                              "activation": "relu",
                              "bias_posterior_fn": null
                            },
                            { "name": "DenseLocalReparametrizationNaturalShared",
                              "units": 10,
                              "activation": "softmax",
                              "bias_posterior_fn": null
                            }
                            ],
                  "prior_scale": 1.0
   },
   "hp": {
          "learning_rate": [7000000],
          "kl_weight": [0.00001],
          "hierarchical": [true, false],
          "scale_init": [[-5, 0.1]],
           "damping_factor": [0.5]
   }
}