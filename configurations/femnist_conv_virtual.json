{
    "session":{
        "num_gpus": 1
    },
    "data_set_conf":{
        "name": "femnist",
        "num_clients": 100,
        "shape": [1, 28, 28]
    },
    "training_conf": {"method": "virtual",
                     "num_rounds": 5000,
                     "clients_per_round": 5,
                     "optimizer": "adam",
                     "fed_avg_init": false,
                     "tensorboard_updates": 1,
                     "damping_factor": 1.0,
                     "kl_weight": 0.0001
    },
   "model_conf": {"layers": [{"name": "Conv2DVirtual",
                              "input_shape": [1, 28, 28],
                              "filters": 64,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu"
                            },
                            { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                            { "name": "Flatten"
                            },
                            { "name": "DenseReparametrizationShared",
                              "units": 10,
                              "activation": "softmax"
                            }
                            ],
                  "hierarchical": false,
                  "prior_scale": 1.0
   },
   "hp": {"epochs_per_round": [1, 5],
         "learning_rate": [0.0005, 0.001, 0.002, 0.005],
         "batch_size": [10, 20]
   }
}