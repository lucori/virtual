{
    "session":{
        "num_gpus": 1,
        "verbose": 0
    },
    "data_set_conf":{
        "name": "femnist",
        "num_clients": 100,
        "shape": [1, 28, 28]
    },
    "training_conf": {"method": "fedprox",
                     "tot_epochs_per_client": 10000,
                     "clients_per_round": 10,
                     "optimizer": "sgd",
                     "tensorboard_updates": 1
    },
   "model_conf": {"layers": [{"name": "Conv2D",
                              "input_shape": [1, 28, 28],
                              "filters": 32,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu"
                            },
                              { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                              {"name": "Conv2D",
                              "input_shape": [1, 28, 28],
                              "filters": 64,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu"
                            },
                            { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                            { "name": "Flatten"
                            },
                            { "name": "Dense",
                              "units": 100,
                              "activation": "relu"
                            },
                            { "name": "Dense",
                              "units": 100,
                              "activation": "relu"
                            },
                            { "name": "Dense",
                              "units": 10,
                              "activation": "softmax"
                            }
                            ]
   },
   "hp": {"epochs_per_round": [1],
         "learning_rate": [0.006, 0.008, 0.03, 0.06, 0.08,
         0.1, 0.01, 0.001, 0.0001, 0.00001],
         "batch_size": [20],
         "l2_reg": [0.0],
         "damping_factor": [1],
         "server_learning_rate": [1]
   }
}