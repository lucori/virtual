{
    "session":{
        "num_gpus": 1
    },
    "data_set_conf":{
        "name": "femnist",
        "num_clients": 100,
        "shape": [1, 28, 28]
    },
    "training_conf": {"method": "fedprox",
                     "tot_epochs_per_client": 5000,
                     "clients_per_round": 5,
                     "optimizer": "adam",
                     "tensorboard_updates": 1
    },
   "model_conf": {"layers": [{"name": "Conv2DCentered",
                              "input_shape": [1, 28, 28],
                              "filters": 32,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu"
                            },
                              { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                              {"name": "Conv2DCentered",
                              "input_shape": [1, 28, 28],
                              "filters": 64,
                              "kernel_size": 5,
                              "padding": "SAME",
                              "activation": "relu"
                            },
                            { "name": "MaxPooling2D",
                              "pool_size": [2, 2],
                              "strides": [2, 2],
                              "padding": "SAME"
                              },
                            { "name": "Flatten"
                            },
                            { "name": "DenseCentered",
                              "units": 100,
                              "activation": "relu"
                            },
                            { "name": "DenseCentered",
                              "units": 100,
                              "activation": "relu"
                            },
                            { "name": "DenseCentered",
                              "units": 10,
                              "activation": "softmax"
                            }
                            ]
   },
   "hp": {"epochs_per_round": [1, 5],
         "learning_rate": [0.0005, 0.001, 0.002, 0.005],
         "batch_size": [10, 20],
         "l2_reg": [0.0, 0.0001, 0.01, 0.1, 1.0]
   }
}